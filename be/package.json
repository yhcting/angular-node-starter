{
  "name": "angular-node-starter",
  "version": "0.1.0",
  "description": "angular-node-starter",
  "scripts": {
    "test": "NODE_ENV=unittest,strict nyc node --nolazy --preserve-symlinks node_modules/.bin/_mocha -t 10000000 $(find build -name \\*.spec.js)",
    "test:rest": "NODE_ENV=resttest,strict node --nolazy --preserve-symlinks node_modules/.bin/_mocha -t 10000000 $(find build/test -name \\*.spec.js)",
    "build": "npm run clean && npm run buildts && npm run tslint && npm run prepare:dev",
    "build:prod": "npm run clean && npm run buildts:prod && npm run tslint:prod && ln -s ../../fe/dist build/public",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:prod": "tslint -c tslint.json -p tsconfig.prod.json",
    "buildts": "node node_modules/.bin/tsc -p tsconfig.json",
    "buildts:prod": "node node_modules/.bin/tsc -p tsconfig.prod.json",
    "clean": "rm -rf build",
    "prepare:dev": "rm -f ../../fe/dist build/public && ln -s ../../fe/dist build/public",
    "postinstall": "script/postinstall.sh",
    "node": "node",
    "start:prod:prof": "NODE_ENV=production node --prof --nolazy --preserve-symlinks build/index.js",
    "start:prod": "NODE_ENV=production node --nolazy --preserve-symlinks build/index.js",
    "start:prof": "NODE_ENV=development,no node --prof --nolazy --preserve-symlinks build/index.js",
    "start": "NODE_ENV=development node --nolazy --preserve-symlinks build/index.js"
  },
  "author": "yhcting",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "hapi": "^17.8.5",
    "hapi-auth-cookie": "^9.1.0",
    "hapi-swagger": "^9.3.0",
    "http-status-codes": "^1.3.0",
    "inert": "^5.1.2",
    "lodash": "^4.17.11",
    "mongodb": "^3.2.5",
    "node": "^10.15.3",
    "rxjs": "^6.3.3",
    "vision": "^5.4.4",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/cls-hooked": "^4.2.1",
    "@types/hapi": "^17.8.6",
    "@types/hapi-auth-cookie": "^9.1.0",
    "@types/inert": "^5.1.2",
    "@types/joi": "^14.0.1",
    "@types/lodash": "^4.14.119",
    "@types/mocha": "^5.2.5",
    "@types/mongodb": "^3.1.27",
    "@types/node": "^10.14.6",
    "@types/node-cache": "^4.1.1",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.15",
    "@types/vision": "^5.3.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "ts-node": "^8.3.0",
    "tslint": "^5.12.0",
    "typescript": "^3.5.2"
  },
  "repository": {
    "type": "git",
    "url": ""
  }
}
